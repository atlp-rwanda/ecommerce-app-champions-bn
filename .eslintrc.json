{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true,
        "mocha": true
    },
    "extends": ["airbnb-base","prettier","plugin:node/recommend"],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier":"error",
        "no-unused-vars":"warn",
        "no-console":"off",
        "func-names":"off",
        "no-process-exit": "off",
        "class-methods-use-this": "off"
    }
}
