{
  "name": "ecommerce-app-champions-bn",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --exec babel-node src",
    "start": "node dist/index.js",
    "build": "babel src -d dist",
    "lint": "esw src --color",
    "lint:fix": "esw --fix",
    "prettier": "npx prettier src test --check",
    "prettier:fix": "npm run prettier -- --write",
    "format": "npm run prettier:fix && npm run lint:fix",
    "lint:watch": "npm run lint -- --watch",
    "migrate:up": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "db:seed": "sequelize-cli seed:generate --name",
    "db:seeds": "sequelize-cli db:seed:all",
    "db:seed:undo": "sequelize-cli db:seed:undo",
    "test": "jest --detectOpenHandles --coverage",
    "docker:compose:build":"docker-compose --build",
    "docker:compose:up":"docker-compose up -d",
    "docker:compose:down":"docker-compose down --volumes --rmi all --remove-orphans",
    "db:create":"npx sequelize db:create"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atlp-rwanda/ecommerce-app-champions-bn.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/atlp-rwanda/ecommerce-app-champions-bn/issues"
  },
  "homepage": "https://github.com/atlp-rwanda/ecommerce-app-champions-bn#readme",
  "dependencies": {
    "@babel/node": "^7.20.7",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "eslint-watch": "^8.0.0",
    "express": "^4.18.2",
    "jest": "^29.4.3",
    "pg": "^8.9.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.29.0",
    "sequelize-cli": "^6.6.0",
    "supertest": "^6.3.3",
    "i18next": "^22.4.10",
    "i18next-fs-backend": "^2.1.1",
    "i18next-http-middleware": "^3.2.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.0",
    "@babel/plugin-transform-runtime": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "eslint": "^8.35.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest-coverage-badge": "^1.0.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.4"
  }
}
