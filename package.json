{
  "name": "ecommerce-app-champions-bn",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --exec babel-node src",
    "start": "node dist/index.js",
    "build": "babel src -d dist",
    "lint": "esw src --color",
    "lint:fix": "esw --fix",
    "prettier": "npx prettier src test --check",
    "prettier:fix": "npm run prettier -- --write",
    "format": "npm run prettier:fix && npm run lint:fix",
    "lint:watch": "npm run lint -- --watch",
    "migrate:up": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "migrate:undo:all": "sequelize-cli db:migrate:undo:all",
    "migrate:up:test": "cross-env NODE_ENV=test npm run migrate:up",
    "migrate:undo:test": "cross-env NODE_ENV=test npm run migrate:undo",
    "migrate:undo:test:all": "cross-env NODE_ENV=test npm run migrate:undo:all",
    "db:seed": "sequelize-cli seed:generate --name",
    "db:seeds": "sequelize-cli db:seed:all",
    "db:seed:undo": "sequelize-cli db:seed:undo",
    "test": "cross-env NODE_ENV=test  npm run migrate:undo:all && cross-env NODE_ENV=test  npm run migrate:up -all && jest --detectOpenHandles --coverage --testTimeout 100000 --forceExit",
    "docker:compose:build": "docker-compose --build",
    "docker:compose:up": "docker-compose up -d",
    "docker:compose:down": "docker-compose down --volumes --rmi all --remove-orphans",
    "db:create": "npx sequelize db:create",
    "db:drop": "npx sequelize db:drop"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atlp-rwanda/ecommerce-app-champions-bn.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/atlp-rwanda/ecommerce-app-champions-bn/issues"
  },
  "homepage": "https://github.com/atlp-rwanda/ecommerce-app-champions-bn#readme",
  "dependencies": {
    "@babel/node": "^7.20.7",
    "bcrypt": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.8",
    "eslint-watch": "^8.0.0",
    "express": "^4.18.2",
    "google-auth-library": "^8.7.0",
    "i18next": "^22.4.10",
    "i18next-fs-backend": "^2.1.1",
    "i18next-http-middleware": "^3.2.2",
    "jest": "^29.4.3",
    "joi": "^17.8.3",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.1",
    "pg": "^8.9.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.29.0",
    "sequelize-cli": "^6.6.0",
    "supertest": "^6.3.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.0",
    "@babel/plugin-transform-runtime": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "eslint": "^8.35.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest-coverage-badge": "^1.0.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.4"
  }
}
